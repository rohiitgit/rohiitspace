{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/server"
    },
    {
      "source": "/health",
      "destination": "/api/server"
    }
  ],
  "headers": [
    {
      "source": "/**/*.{css,js,png,jpg,jpeg,gif,svg,ico,pdf}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}