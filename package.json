{
  "name": "rohiitspace",
  "version": "1.0.0",
  "description": "Rohit's portfolio website with Spotify integration",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server -p 3000 -c-1",
    "dev": "npx http-server -p 3000 -c-1",
    "build": "npx @tailwindcss/cli -i ./styles.css -o ./public/src/styles/tailwind.css --minify && echo 'Build completed - static files ready'",
    "build:css": "npx @tailwindcss/cli -i ./styles.css -o ./public/src/styles/tailwind.css",
    "build:css:watch": "npx @tailwindcss/cli -i ./styles.css -o ./public/src/styles/tailwind.css --watch",
    "vercel-dev": "vercel dev"
  },
  "dependencies": {
    "@vercel/analytics": "^1.3.1",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.19.2",
    "lenis": "^1.3.13"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "vercel": "^50.18.0"
  },
  "keywords": [
    "portfolio",
    "spotify",
    "music",
    "frontend",
    "serverless"
  ],
  "author": "rohit",
  "license": "MIT"
}
